{
  "entryFile": "src/server.ts",
  "noImplicitAdditionalProperties": "throw",
  "controllerPathGlobs": ["src/controllers/**/*.ts"],
  "spec": {
    "outputDirectory": "src/swagger",
    "specVersion": 3,
    "securityDefinitions": {
      "api_key": {
        "type": "apiKey",
        "name": "X-API-Key",
        "in": "header"
      }
    },
    "info": {
      "title": "iPlayarr",
      "version": "1.0.0",
      "description": "API Used for querying internal types from iPlayarr."
    }
  }
}
